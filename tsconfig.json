{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "importsNotUsedAsValues": "error",
    "moduleResolution": "node",
    "paths": {
      "@components/*": ["src/components/*"],
      "@dataset": ["src/dataset.ts"],
      "@styles/*": ["src/styles/*"],
      "@types": ["src/types.ts"]
    },
    "types": ["astro/client", "node"]
  },
  "include": ["src/*.d.ts"]
}
